{
  "name": "@mfe-brasil/single-spa-devtools",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:vscode": "bunx nx run vscode-extension:compile",
    "watch:vscode": "bunx nx run vscode-extension:watch",
    "package:vscode": "bunx nx run vscode-extension:package",
    "lint:vscode": "bunx nx run vscode-extension:lint",
    "test:vscode": "bunx nx run vscode-extension:test",
    "build:webpack-plugin": "bunx nx run webpack-plugin:build",
    "lint:webpack-plugin": "bunx nx run webpack-plugin:lint",
    "test:webpack-plugin": "bunx nx run webpack-plugin:test",
    "lint": "bunx nx run-many --target=lint",
    "format": "bunx nx run-many --target=format"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@nx/js": "21.0.3",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.11.24",
    "@swc/helpers": "~0.5.17",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "biome": "^0.3.3",
    "nx": "21.0.3",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "workspaces": [
    "packages/*"
  ]
