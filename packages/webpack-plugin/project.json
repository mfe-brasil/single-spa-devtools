{
  "name": "webpack-plugin",
  "root": "packages/webpack-plugin",
  "projectType": "library",
  "sourceRoot": "packages/webpack-plugin/src",
  "tags": ["webpack", "plugin"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --outDir dist"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check src"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No tests yet'"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome format src --write"
      }
    }
  }
} 